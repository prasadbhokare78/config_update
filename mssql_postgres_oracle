insert into datalake_source_tracker(source_name, database_type, database_name, table_name, row_count, date_column, fetch_type, hour_interval, mode, batch_size, created_at, updated_at, flag, executor_memory, executor_cores,  driver_memory, min_executors, max_executors, initial_executors, driver_cores, add_date_column, mod_date_column, min_date_column) values ('tcs_back_office', 'oracle', 'fin', 'void_1', 10, '2025-02-21', 'batch', 4, 'append', 1000, '2025-02-26', '2025-02-26', 'true', 2,2,2,2,2,2,2 , 'updated_at', 'updated_at','updated_at');


update datalake_source_tracker set date_column = 'updated_at', fetch_type = 'batch', hour_interval = 2, mode = 'append', batch_size = 10000, created_at = '2025-02-27', updated_at = '2025-02-28', flag = 'true', executor_memory = 2, executor_cores = 2, driver_memory = 2, min_executors = 2, max_executors = 2, initial_executors = 2, driver_cores = 2, mod_date_column = 'mod_at', add_date_column = 'mod_at', min_date_column = 'min_at' where database_type IN ('oracle', 'mssql', 'postgresql');


select * from config_execution_log c1 where last_execution_date = (select max(last_execution_date) from config_execution_log c2 where c1.source_name = c2.source_name and c1.database_name = c2.database_name and c1.database_type = c2.database_type);

